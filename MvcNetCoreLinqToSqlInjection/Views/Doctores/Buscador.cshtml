@model List<Doctor>

    <h1>Doctores Especialidad</h1>
    <form method="post">
        <label>Introdzca una Especialidad</label>
        <input type="text" name="busqueda" class="form-control"/>
        <button> Buscador Doctores</button>
    </form>


    <table>
        <thead>
            <tr>
                <th>Apellido</th>
                <th>Especialidad</th>
                <th>Salario</th>
            </tr>
        </thead>
        <tbody>
        @foreach(Doctor doctor in Model)
        {
            <tr>
                <td>@doctor.Apellido</td>
                <td>@doctor.Especialidad</td>
                <td>@doctor.Salario</td>
            </tr>
        }
        </tbody>
    </table>